<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
  "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
  "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

  <include file="dhis-web-commons.xml" />

  <package name="jphes-web-app" extends="dhis-web-commons" namespace="/jphes-web-app">

    <!-- JPHES Web APP -->

    <action name="index" class="org.hisp.dhis.partner.action.NoAction">
      <result name="success" type="velocity">/main.vm</result>
      <param name="page">/jphes-web-app/index.vm</param>
      <param name="menu">/jphes-web-app/menu.vm</param>
    </action>

    <action name="allPartner" class="org.hisp.dhis.partner.action.GetPartnerListAction">
      <result name="success" type="velocity">/main.vm</result>
      <param name="page">/jphes-web-app/allPartner.vm</param>
      <param name="menu">/jphes-web-app/menu.vm</param>
      <param name="javascripts">javascript/partner.js,javascript/filterTable.js</param>
      <!--<param name="requiredAuthorities">F_JPHES_LIST_PARTNER</param>-->
    </action>

    <action name="index" class="org.hisp.dhis.partner.action.NoAction">
      <result name="success" type="velocity">/main.vm</result>
      <param name="page">/jphes-web-app/index.vm</param>
      <param name="menu">/jphes-web-app/menu.vm</param>
    </action>
    <action name="showAddRoleForm" class="org.hisp.dhis.user.action.NoAction">
      <result name="success" type="velocity">/main.vm</result>
      <param name="page">/dhis-web-maintenance-user/addRoleForm.vm</param>
      <param name="javascripts">../dhis-web-commons/oust/oust.js,javascript/role.js</param>
      <param name="anyAuthorities">F_USERROLE_PUBLIC_ADD, F_USERROLE_PRIVATE_ADD</param>
    </action>

  </package>
</struts>
